<h1>Add new user</h1>


<%= form_with model: @user do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <%= f.label :date_of_birth %>
  <%= f.date_field :date_of_birth %>
  <%= f.label :gender %>
  <%= f.text_field :gender %>
  <%= f.submit 'Create User' %>
<% end %>
